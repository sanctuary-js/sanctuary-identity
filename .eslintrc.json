{
  "root": true,
  "extends": ["./node_modules/sanctuary-style/eslint-es3.json"],
  "overrides": [
    {
      "files": ["*.md"],
      "globals": {"$": false, "Identity": false, "S": false, "Z": false, "show": false, "type": false}
    },
    {
      "files": ["index.js"],
      "rules": {
        "multiline-comment-style": ["off"],
        "no-unused-vars": ["error", {"varsIgnorePattern": "^S$"}]
      }
    }
  ]
}
